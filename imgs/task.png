<script>
function changeImageFilePath ( html ) {  
  var imagePath = document.getElementById('imagePath');
  html = html.replace(/img src="([^:])/g,'img src="'+imagePath.value+'$1');
  html = html.replace(/img src=":/g,'img src="');
  var filePath = document.getElementById('filePath');
  html = html.replace(/a href="([^:])/g,'a href="'+filePath.value+'$1');
  html = html.replace(/a href=":/g,'a href="');
  return html;
}
function showDiv(id) {
  var hideData = document.getElementById(id);
  var pRef = document.getElementById('p' + id);
  var refText = pRef.innerHTML;
//  if ( refText.startsWith('Показать') ) {   
